function kernel(a, b) {
    local sum = 0
    for (local i = 0; i < 2; i = i + 1) {
        sum = sum + a[this.thread.y, i] * b[i, this.thread.x]
    }
    return(sum)
}

multiplyMatrices = gpu.device.createKernel(kernel)
multiplyMatrices.setOutput([2, 2])

a = [1,2;3,4]
b = [5,6;7,8]

c = multiplyMatrices(a, b)

system.log(c)
